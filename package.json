{
  "name": "henrique-fonseca-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env DOTENV_CONFIG_PATH='./config/.env.development' next dev -p 3001",
    "build": "next build",
    "start": "cross-env DOTENV_CONFIG_PATH=.env.production next start -p 3001",
    "prisma:dev": "cross-env DOTENV_CONFIG_PATH=.env.development npx prisma generate",
    "migrate": "prisma migrate dev",
    "migrate:prod": "prisma migrate deploy",
    "load-mockdata": "node ./development/scripts/loadMockData.js",
    "forceLoad-mockdata": "node ./development/scripts/forceLoadMockData.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write '**/*.{js,json,jsx,md,yml,yaml,json,css}'",
    "check-unused": "depcheck",
    "lint": "eslint .",
    "clean": "rimraf .next"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.1",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@prisma/client": "^5.2.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint-config-next": "13.4.19",
    "firebase": "^10.12.2",
    "html-react-parser": "^5.1.10",
    "jest": "^29.7.0",
    "lucide-react": "^0.399.0",
    "mongodb": "^6.8.0",
    "next": "^14.2.4",
    "next-auth": "^4.23.1",
    "prisma": "^5.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-quill": "^2.0.0",
    "swr": "^2.2.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.0",
    "@eslint/js": "^9.6.0",
    "depcheck": "^1.4.7",
    "eslint-plugin-react": "^7.34.3",
    "globals": "^15.6.0",
    "prettier": "^3.3.2"
  }
}
